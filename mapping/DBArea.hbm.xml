		<property name="Y" type="Int32" not-null="true" column="`Y`"/>
		<property name="Z" type="Int32" not-null="true" column="`Z`"/>
		<property name="Radius" type="Int32" not-null="true" column="`Radius`"/>
		<property name="Region" type="Int32" not-null="true" column="`Region`"/>
		<property name="ClassType" type="String(255)" not-null="true" column="`ClassType`"/>
		<property name="CanBroadcast" type="YesNo" not-null="true" column="`CanBroadcast`"/>
		<property name="Sound" type="Byte" not-null="true" column="`Sound`"/>
	</class>
</hibernate-mapping>
