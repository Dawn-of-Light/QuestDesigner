<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.GS.Database.DBQuest, GameServer" table="`Quest`">
		<id name="QuestID" type="Int32" column="`QuestID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Name" type="String(255)" not-null="true" column="`Name`"/>
		<property name="CharacterID" type="Int32" not-null="true" column="`CharacterID`"/>
		<property name="Step" type="Int32" not-null="true" column="`Step`"/>
		<property name="CustomPropertiesString" type="String(255)" not-null="true" column="`CustomPropertiesString`"/>	
	</class>
</hibernate-mapping>
