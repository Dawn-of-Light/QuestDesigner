<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.GS.Database.DBNpcTemplate, GameServer" table="`NpcTemplate`">
		<id name="NpcTemplateID" column="`NpcTemplateID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Name" type="String(255)" not-null="true" column="`Name`"/>
		<property name="GuildName" type="String(255)" not-null="true" column="`GuildName`"/>
		<property name="Model" type="Int32" not-null="true" column="`Model`"/>
		<property name="Size" type="Byte" not-null="true" column="`Size`"/>
		<property name="MaxSpeed" type="Int16" not-null="true" column="`MaxSpeed`"/>
		<property name="EquipmentTemplateID" type="String(255)" not-null="true" column="`EquipmentTemplateID`"/>
		<property name="Ghost" type="YesNo" not-null="true" column="`Ghost`"/>
		<property name="MeleeDamageType" type="Byte" not-null="true" column="`MeleeDamageType`"/>
		<property name="ParryChance" type="Byte" not-null="true" column="`ParryChance`"/>
		<property name="EvadeChance" type="Byte" not-null="true" column="`EvadeChance`"/>
		<property name="BlockChance" type="Byte" not-null="true" column="`BlockChance`"/>
		<property name="LeftHandSwingChance" type="Byte" not-null="true" column="`LeftHandSwingChance`"/>
		<property name="Spells" type="String(255)" not-null="true" column="`Spells`"/>
		<property name="Styles" type="String(255)" not-null="true" column="`Styles`"/>
	</class>
</hibernate-mapping>
