<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.GS.Database.DBAlliance, GameServer" table="`Alliance`">
		<id name="AllianceName" type="String(255)" column="`AllianceName`">
			<generator class="assigned" />
		</id>
		<property name="Motd" not-null="true" column="`Motd`"/>
		<many-to-one name="DBguildleader" column="`GuildName`" class="DOL.GS.Database.DBGuild, GameServer" />
		<array name="DBguilds" table="`Guild`">
			<key column="`AllianceName`"/>
			<index column="`BountyPoints`" type="Int32"/>
			<one-to-many class="DOL.GS.Database.DBGuild, GameServer"/>
		</array>
		
	</class>
</hibernate-mapping>
